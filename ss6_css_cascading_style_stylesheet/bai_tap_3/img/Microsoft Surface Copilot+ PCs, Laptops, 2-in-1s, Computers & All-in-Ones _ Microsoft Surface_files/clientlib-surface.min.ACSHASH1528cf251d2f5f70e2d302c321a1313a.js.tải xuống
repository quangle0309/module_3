'use strict';$(document).ready(function(c){0<$("meta[name\x3d'uhf-ctaReplace']").length?(c=$("meta[name\x3d'uhf-ctaReplace']").attr("content").split(","),$("#where-to-buy").attr({href:c[2],"aria-label":c[1]}).text(c[0]).css("visibility","visible")):0<$("meta[name\x3d'uhf-ctaRemove']").length?$("#where-to-buy").css("visibility","hidden"):$("#where-to-buy").css("visibility","visible");$(".animation-text").length&&$(".animation-text .context-accessory \x3e div \x3e div, .animation-text .container, .animation-text .card").each(function(b,
a){function d(){$(a).find("div").animate({top:"0px"},750)}$(this).css("overflow","hidden");if($(a).offset().top+$(a).height()>=$(window).scrollTop())if($(a).find("div").css("top",$(a).height()),$(a).offset().top+$(a).height()<$(window).scrollTop()+$(window).height())d();else $(window).on("scroll",function(){$(a).offset().top+$(a).height()<$(window).scrollTop()+$(window).height()&&d()})});$(".show-more-show-less").on("click",".show-more-show-less-toggle[aria-expanded\x3d'false']",function(){var b=
$(".show-more-show-less-toggle[aria-expanded\x3d'false']").offset().top;$("html,body").animate({scrollTop:b-100+"px"},"fast")});$(".show-more-show-less").on("keydown",".show-more-show-less-toggle[aria-expanded\x3d'false']",function(b){13===b.keyCode&&(b=$(".show-more-show-less-toggle[aria-expanded\x3d'false']").offset().top,$("html,body").animate({scrollTop:b-100+"px"},"fast"))})});