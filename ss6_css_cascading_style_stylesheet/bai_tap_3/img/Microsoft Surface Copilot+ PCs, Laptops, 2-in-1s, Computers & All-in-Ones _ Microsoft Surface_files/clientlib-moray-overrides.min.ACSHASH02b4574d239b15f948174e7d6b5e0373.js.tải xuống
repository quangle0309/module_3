'use strict';document.addEventListener("DOMContentLoaded",function(){document.querySelectorAll("moray-link-group").forEach(function(a){var b=a.getAttribute("data-link-group-color"),c=a.getAttribute("data-button-text-color");b&&(a.style.setProperty("--ds-button-background-color",b),a.style.setProperty("--ds-button-hover-background-color",b),a.style.setProperty("--ds-anchor-color",b),a.style.setProperty("--ds-theme-foreground-accent-subtle-normal",b));c&&a.style.setProperty("--ds-button-color",c)})});